#!/bin/sh

dumpDb()
{
    db="$1"
    echo ".dump" | sqlite3 "$db"
}

ancestor="$(dumpDb "$1")"
local="$(dumpDb "$2")"
remote="$(dumpDb "$3")"
marker="$4"
placeholder="$5"

echo "ancestor='$ancestor'"
echo "local='$local'"
echo "remote='$remote'"
echo "marker='$marker'"
echo "placeholder='$placeholder'"

exit 1;
